e=\"text\"\r\n        :value=\"todo.title\"\r\n        @blur=\"handleFinishEdit(todo, $event)\"\r\n        @keyup.enter=\"handleFinishEdit(todo, $event)\"\r\n        ref=\"inputTitle\"\r\n      />\r\n    </label>\r\n    <button class=\"btn btn-danger\" @click=\"handleDelete(todo.id)\">删除</button>\r\n    <button\r\n      v-show=\"!todo.isEdit\"\r\n      class=\"btn btn-edit\"\r\n      @click=\"handleEdit(todo.id)\"\r\n    >\r\n      编辑\r\n    </button>\r\n  </li>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"TodoItem\",\r\n  props: [\"todo\"],\r\n  methods: {\r\n    handleCheck(todoId) {\r\n      this.$bus.$emit(\"checkTodo\", todoId);\r\n    },\r\n    handleDelete(todoId) {\r\n      if (confirm(\"真的要删除此事件吗？(OAO)!\")) {\r\n        this.$bus.$emit(\"deleteTodo\", todoId);\r\n      }\r\n    },\r\n    handleEdit(todoId) {\r\n      this.$bus.$emit(\"editTodo\", todoId);\r\n      this.$nextTick(function () {\r\n        this.$refs.inputTitle.focus();\r\n      });\r\n    },\r\n    handleFinishEdit(todo, e) {\r\n      if (!e.target.value.trim()) {\r\n        this.$bus.$emit(\"updateTodo\", todo.id, todo.title);\r\n        return alert(\"输入不能为空噢^v^\");\r\n      }\r\n      this.$bus.$emit(\"updateTodo\", todo.id, e.target.value);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/*item*/\r\nli {\r\n  list-style: none;\r\n  height: 36px;\r\n  line-height: 36px;\r\n  padding: 0 5px;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\nli label {\r\n  float: left;\r\n  cursor: pointer;\r\n}\r\n\r\nli label li input {\r\n  vertical-align: middle;\r\n  margin-right: 6px;\r\n  position: relative;\r\n  top: -1px;\r\n}\r\n\r\nli button {\r\n  float: right;\r\n  display: none;\r\n  margin-top: 3px;\r\n}\r\n\r\nli:before {\r\n  content: initial;\r\n}\r\n\r\nli:last-child {\r\n  border-bottom: none;\r\n}\r\n\r\nli:hover {\r\n  background-color: #ddd;\r\n}\r\n\r\nli:hover button {\r\n  display: block;\r\n}\r\n</style>"]}]}