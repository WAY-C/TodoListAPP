A;AACA;AAlBA;AAPA,CAAA","sourcesContent":["<template>\r\n  <div class=\"todo-header\">\r\n    <input\r\n      type=\"text\"\r\n      placeholder=\"请输入你的todo名称，按回车键确认\"\r\n      v-model=\"title\"\r\n      @keyup.enter=\"addTodo\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { nanoid } from \"nanoid\";\r\nexport default {\r\n  name: \"TodoHeader\",\r\n  data() {\r\n    return {\r\n      title: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    addTodo() {\r\n      //如果输入为空\r\n      if (!this.title.trim()) {\r\n        this.title = \"\";\r\n        return alert(\"输入不能为空噢^v^\");\r\n      }\r\n      // 将用户输入包装成一个todo对象\r\n      const todoObj = {\r\n        id: nanoid(),\r\n        title: this.title,\r\n        done: false,\r\n        isEdit: false,\r\n      };\r\n      // 通知APP组件去添加一个todo对象\r\n      this.$emit(\"addNewTodo\", todoObj);\r\n      // 输入置为空\r\n      this.title = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/*header*/\r\n.todo-header input {\r\n  width: 560px;\r\n  height: 28px;\r\n  font-size: 14px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  padding: 4px 7px;\r\n}\r\n\r\n.todo-header input:focus {\r\n  outline: none;\r\n  border-color: rgba(82, 168, 236, 0.8);\r\n  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075),\r\n    0 0 8px rgba(82, 168, 236, 0.6);\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}