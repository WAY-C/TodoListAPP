ACA,IAAA,QAFA,sBAEA;AACA,WAAA,QAAA,GAAA,MAAA,CAAA,UAAA,CAAA,IAAA,KAAA,QAAA;AACA,KAJA;AAKA;AACA,IAAA,UANA,sBAMA,UANA,EAMA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA;AACA,KARA;AASA;AACA,IAAA,SAVA,qBAUA,EAVA,EAUA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,EAAA,KAAA,EAAA,EAAA;AACA,UAAA,IAAA,CAAA,IAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,OAJA;AAKA,KAhBA;AAiBA;AACA,IAAA,UAlBA,sBAkBA,EAlBA,EAkBA;AACA,WAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,EAAA;AAAA,OAAA,CAAA;AACA,KApBA;AAqBA;AACA,IAAA,YAtBA,wBAsBA,IAtBA,EAsBA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AAAA,OAAA;AACA,KAxBA;AAyBA;AACA,IAAA,YA1BA,0BA0BA;AACA,WAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,CAAA,IAAA,CAAA,IAAA;AAAA,OAAA,CAAA;AACA,KA5BA;AA6BA;AACA,IAAA,QA9BA,oBA8BA,MA9BA,EA8BA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,EAAA,KAAA,MAAA,EAAA;AACA,UAAA,IAAA,CAAA,MAAA,GAAA,IAAA;AACA;AACA,OAJA;AAKA,KApCA;AAqCA;AACA,IAAA,UAtCA,sBAsCA,MAtCA,EAsCA,KAtCA,EAsCA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,EAAA,KAAA,MAAA,EAAA;AACA,UAAA,IAAA,CAAA,KAAA,GAAA,KAAA;AACA,UAAA,IAAA,CAAA,MAAA,GAAA,KAAA;AACA;AACA,OALA;AAMA;AA7CA,GAhBA;AA+DA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,IADA;AAEA,MAAA,OAFA,mBAEA,KAFA,EAEA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA;AACA;AAJA;AADA,GA/DA;AAuEA,EAAA,OAvEA,qBAuEA;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,WAAA,EAAA,KAAA,SAAA;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,UAAA;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,UAAA,EAAA,KAAA,QAAA;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,UAAA;AACA,GA5EA;AA6EA,EAAA,aA7EA,2BA6EA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,WAAA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,YAAA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,YAAA;AACA;AAlFA,CAAA","sourcesContent":["<template>\n  <div id=\"root\">\n    <div class=\"todo-container\">\n      <div class=\"todo-wrap\">\n        <header>\n          <h1 @click=\"editUser\">{{ userName }}'s TodoList</h1>\n        </header>\n        <!-- 此处添加一个自定义事件addNewTodo，后者是回调函数addNewTodo，二者可以同名 -->\n        <TodoHeader @addNewTodo=\"addNewTodo\" />\n        <TodoList :todos=\"todos\" />\n        <TodoFooter\n          :todos=\"todos\"\n          :checkAllTodo=\"checkAllTodo\"\n          :clearAllTodo=\"clearAllTodo\"\n        />\n        <h4>&emsp;使用说明：</h4>\n        <ul>\n          <li>在文本框中输入文本来添加一个todo</li>\n          <li>勾选或取消勾选todo前的方框来切换“已完成”、“未完成”</li>\n          <li>单击todo后的“删除”按钮来删除一个todo</li>\n          <li>\n            单击todo后的“编辑”按钮来编辑当前todo，<br />单击修改框外任意位置或单击回车提交\n          </li>\n          <li>单击全选方框来选择或取消选择全部todo</li>\n          <li>单击“清除已完成任务”来删除已完成（已选中）的todo</li>\n          <li>支持同一浏览器下的本地保存</li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { nanoid } from \"nanoid\";\nimport TodoHeader from \"./components/TodoHeader\";\nimport TodoList from \"./components/TodoList\";\nimport TodoFooter from \"./components/TodoFooter\";\n\nexport default {\n  name: \"App\",\n  components: { TodoHeader, TodoList, TodoFooter },\n  data() {\n    return {\n      todos: JSON.parse(localStorage.getItem(\"todos\")) || [\n        {\n          id: nanoid(),\n          title: \"拥有好心情\",\n          done: false,\n          isEdit: false,\n        },\n      ],\n      userName: \"Waylon\",\n    };\n  },\n  methods: {\n    //单击header标签中的文本时修改用户名\n    editUser() {\n      this.userName = prompt(\"请输入你的名字：\") || this.userName;\n    },\n    // 自定义事件addNewTodo的回调函数，添加一个todo\n    addNewTodo(newTodoObj) {\n      this.todos.unshift(newTodoObj);\n    },\n    // 勾选与取消勾选修改todo的done属性值\n    checkTodo(id) {\n      this.todos.forEach((todo) => {\n        if (todo.id === id) {\n          todo.done = !todo.done;\n        }\n      });\n    },\n    // 删除一个todo\n    deleteTodo(id) {\n      this.todos = this.todos.filter((todo) => todo.id !== id);\n    },\n    // 全选或全不选\n    checkAllTodo(done) {\n      this.todos.forEach((todo) => (todo.done = done));\n    },\n    // 点击footer中清除已选事件时删除相应todo\n    clearAllTodo() {\n      this.todos = this.todos.filter((todo) => !todo.done);\n    },\n    // 点击Item上编辑按钮时改变todo的isEdit属性\n    editTodo(todoId) {\n      this.todos.forEach((todo) => {\n        if (todo.id === todoId) {\n          todo.isEdit = true;\n        }\n      });\n    },\n    // 编辑完成时\n    updateTodo(todoId, title) {\n      this.todos.forEach((todo) => {\n        if (todo.id === todoId) {\n          todo.title = title;\n          todo.isEdit = false;\n        }\n      });\n    },\n  },\n  watch: {\n    todos: {\n      deep: true,\n      handler(value) {\n        localStorage.setItem(\"todos\", JSON.stringify(value));\n      },\n    },\n  },\n  mounted() {\n    this.$bus.$on(\"checkTodo\", this.checkTodo);\n    this.$bus.$on(\"deleteTodo\", this.deleteTodo);\n    this.$bus.$on(\"editTodo\", this.editTodo);\n    this.$bus.$on(\"updateTodo\", this.updateTodo);\n  },\n  beforeDestroy() {\n    this.$bus.$off(\"checkTodo\");\n    this.$bus.$off(\"deleteTodo\");\n    this.$bus.$off(\"editTodo\");\n    this.$bus.$off(\"updateTodo\");\n  },\n};\n</script>\n\n<style>\n/*base*/\nbody {\n  background: #fff;\n}\n\nheader {\n  text-align: center;\n}\nheader:hover {\n  cursor: pointer;\n}\n\n.btn {\n  display: inline-block;\n  padding: 4px 12px;\n  margin-bottom: 0;\n  font-size: 14px;\n  line-height: 20px;\n  text-align: center;\n  vertical-align: middle;\n  cursor: pointer;\n  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2),\n    0 1px 2px rgba(0, 0, 0, 0.05);\n  border-radius: 4px;\n}\n\n.btn-danger {\n  color: #fff;\n  background-color: #da4f49;\n  border: 1px solid #bd362f;\n}\n\n.btn-edit {\n  color: #fff;\n  background-color: skyblue;\n  border: 1px solid rgb(15, 159, 216);\n  margin-right: 5px;\n}\n.btn-edit:hover {\n  color: #fff;\n  background-color: rgb(102, 157, 180);\n}\n.btn-danger:hover {\n  color: #fff;\n  background-color: #bd362f;\n}\n\n.btn:focus {\n  outline: none;\n}\n\n.todo-container {\n  width: 600px;\n  margin: 0 auto;\n}\n.todo-container .todo-wrap {\n  padding: 10px;\n  border: 1px solid #ddd;\n  border-radius: 5px;\n}\n</style>\n"],"sourceRoot":"src"}]}